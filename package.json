{
	"name": "@igor.dvlpr/windev",
	"version": "1.1.1",
	"description": "🍃 Provides ways of checking whether a path is a legacy Windows device. 💾",
	"main": "./dist/cjs/main.js",
	"module": "./dist/esm/main.mjs",
	"types": "./dist/types/main.d.ts",
	"exports": {
		"import": "./dist/esm/main.mjs",
		"require": "./dist/cjs/main.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build:esm": "babel src --out-dir dist/esm --keep-file-extension",
		"build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir dist/cjs",
		"dts": "tsc",
		"build": "npm run dts && npm run build:esm && npm run build:cjs",
		"test": "npm run build:esm && npm run build:cjs && mocha",
		"pub": "npm run dts && npm test && npm publish --access public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/igorskyflyer/npm-windev.git"
	},
	"keywords": [
		"windows",
		"legacy",
		"device",
		"path",
		"check",
		"utility",
		"filesystem",
		"igor.dvlpr"
	],
	"author": "Igor Dimitrijević <igor.dvlpr@gmail.com> (https://github.com/igorskyflyer)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/igorskyflyer/npm-windev/issues"
	},
	"homepage": "https://github.com/igorskyflyer/npm-windev#readme",
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.14.8",
		"@babel/preset-env": "^7.14.8",
		"chai": "^4.3.4",
		"cross-env": "^7.0.3",
		"mocha": "^9.0.3",
		"typescript": "^4.3.5"
	}
}
