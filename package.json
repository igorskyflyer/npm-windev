{
    "name": "@igorskyflyer/windev",
    "version": "1.3.0",
    "description": "🍃 Determines whether a path is a legacy Windows device. 💾",
    "sideEffects": false,
    "engines": {
        "node": ">=22.0"
    },
    "type": "module",
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "test": "vitest --run",
        "pub": "pnpm test && git push origin main --tags"
    },
    "keywords": [
        "windows-device-check",
        "legacy-windows-device",
        "reserved-filenames",
        "windows-filenames",
        "file-name-validation",
        "path-validation",
        "windows-path-check",
        "floppy-disk",
        "dos-device-names",
        "file-system-utils",
        "windows-utils",
        "typescript",
        "ts",
        "igor dimitrijević",
        "igorskyflyer"
    ],
    "license": "MIT",
    "author": "Igor Dimitrijević <support@igorskyflyer.me> (https://github.com/igorskyflyer)",
    "funding": "https://ko-fi.com/igorskyflyer",
    "homepage": "https://github.com/igorskyflyer/npm-windev",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/igorskyflyer/npm-windev.git"
    },
    "bugs": {
        "url": "https://github.com/igorskyflyer/npm-windev/issues"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.2.2",
        "@igorskyflyer/biome-config": "^2.1.0",
        "@types/node": "^22.18.0",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "publishConfig": {
        "access": "public"
    },
    "packageManager": "pnpm@10.15.0"
}
