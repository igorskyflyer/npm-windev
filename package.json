{
	"name": "@igor.dvlpr/windev",
	"version": "1.1.3",
	"description": "🍃 Provides ways of checking whether a path is a legacy Windows device. 💾",
	"type": "module",
	"sideEffects": false,
	"main": "./dist/lib/main.cjs",
	"module": "./src/main.js",
	"types": "./dist/types/main.d.ts",
	"exports": {
		"import": "./src/main.js",
		"require": "./dist/lib/main.cjs"
	},
	"files": [
		"src",
		"dist"
	],
	"scripts": {
		"build:cjs": "babel src/main.js --out-file dist/lib/main.cjs",
		"dts": "tsc",
		"build": "npm run build:cjs && npm run dts",
		"test": "mocha",
		"pub": "npm test && npm run build && npm publish --access public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/igorskyflyer/npm-windev.git"
	},
	"keywords": [
		"windows",
		"legacy",
		"device",
		"path",
		"check",
		"utility",
		"filesystem",
		"igor.dvlpr"
	],
	"author": "Igor Dimitrijević <igor.dvlpr@gmail.com> (https://github.com/igorskyflyer)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/igorskyflyer/npm-windev/issues"
	},
	"homepage": "https://github.com/igorskyflyer/npm-windev#readme",
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.14.8",
		"@babel/plugin-transform-modules-commonjs": "^7.14.5",
		"@babel/preset-env": "^7.14.8",
		"@types/chai": "^4.2.21",
		"@types/mocha": "^9.0.0",
		"chai": "^4.3.4",
		"mocha": "^9.0.3"
	}
}
